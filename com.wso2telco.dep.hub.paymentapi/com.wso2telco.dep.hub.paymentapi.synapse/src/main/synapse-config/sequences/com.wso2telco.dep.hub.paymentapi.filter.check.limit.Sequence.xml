<?xml version="1.0" encoding="UTF-8"?>
<sequence
    name="com.wso2telco.dep.hub.paymentapi.filter.check.limit.Sequence" xmlns="http://ws.apache.org/ns/synapse">
    <filter regex="Postpaid|Prepaid"
        source="get-property('userpackagetype')" xmlns:ns="http://org.apache.synapse/xsd">
        <then>
            <sequence key="com.wso2telco.dep.hub.paymentapi.check.spendlimit.Sequence"/>
        </then>
        <else/>
    </filter>
</sequence>
